{
  "name": "webpage-capture-tool",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo for the webpage capture CLI and Electron app",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "cli": "npm run cli --workspace @webpage-capture/cli --",
    "start": "npm run start --workspace @webpage-capture/electron-app",
    "setup": "npm run setup --workspace @webpage-capture/electron-app",
    "setup:bundle": "npm run setup:bundle --workspace @webpage-capture/electron-app",
    "build:mac": "npm run build:mac --workspace @webpage-capture/electron-app",
    "build:win": "npm run build:win --workspace @webpage-capture/electron-app"
  },
  "dependencies": {
    "electron": "31.3.1"
  }
}
